{
  "title": "open2",
  "maintainers": [
    "annietma"
  ],
  "rules": [
    {
      "description": "Command+Shift+Q: original âŒ˜+Q behavior",
      "manipulators": [
        {
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": ["right_command", "shift"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "q",
              "modifiers": ["right_command", "shift"]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Command+Q to open Slack",
      "manipulators": [
        {
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "shell_command": "open -a Slack"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Command+P to open Google Chrome",
      "manipulators": [
        {
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "shell_command": "open -a 'Google Chrome'"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Command+U to open WebStorm",
      "manipulators": [
        {
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "shell_command": "open -a WebStorm"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Command+Y to open ChatGPT",
      "manipulators": [
        {
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "shell_command": "open -a ChatGPT"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
